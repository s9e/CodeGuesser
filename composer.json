	"homepage": "https://github.com/s9e/CodeGuesser/",
	"keywords": ["code"],
	"license": "MIT",
	"require": {
		"php": ">=8.1"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.5"
	},
	"autoload": {
